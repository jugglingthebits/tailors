@model Tailors.Web.Features.Feed.FeedViewModel

@foreach (var tweed in Model.Tweeds)
{
    <partial name="_Tweed" model="tweed"/>
}

@if (Model.NextPageExists)
{
    <div hx-get="@Url.Action("Feed", new { page = Model.Page + 1 })"
         hx-trigger="revealed"
         hx-swap="beforebegin">
    </div>
}
